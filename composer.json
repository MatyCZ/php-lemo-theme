{
    "name": "matycz/lemo-theme",
    "description": "",
    "license": "BSD-3-Clause",
    "type": "library",
    "keywords": [
        "laminas",
        "theme"
    ],
    "homepage": "https://github.com/MatyCZ/php-lemo-theme",
    "autoload": {
        "psr-4": {
            "Lemo\\Theme\\": "src/"
        }
    },
    "extra": {
        "laminas": {
            "component": "Lemo\\Theme",
            "config-provider": "Lemo\\Theme\\ConfigProvider"
        }
    },
    "require": {
        "laminas/laminas-dependency-plugin": "^2.0",
        "laminas/laminas-eventmanager": "^3.0",
        "laminas/laminas-mvc": "^3.0",
        "laminas/laminas-servicemanager": "^3.0",
        "laminas/laminas-stdlib": "^3.0",
        "laminas/laminas-view": "^2.0",
        "php": "^7.4 || ^8.0"
    },
    "require-dev": {
        "vimeo/psalm": "~4.7.0"
    },
    "scripts": {
        "analyse-psalm": "psalm"
    }
}
